<html>

<head>
    <title>
        Dashboard page
    </title>
    <meta charset='utf-8' />

    <link rel='apple-touch-icon' sizes='180x180' href='/fav/apple-touch-icon.png' />
    <link rel='icon' type='image/png' sizes='32x32' href='/fav/favicon-32x32.png' />
    <link rel='icon' type='image/png' sizes='16x16' href='/fav/favicon-16x16.png' />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link rel='mask-icon' href='/fav/safari-pinned-tab.svg' color='#5bbad5' />
    <meta name='msapplication-TileColor' content='#da532c' />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta name="viewport"
        content="viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">

<body>

    <div class="page page--dashboard">

        <aside class="page__aside aside vertical vertical--big">
            <div class="text aside__logo">Sidebar</div>
            <nav class="vertical vertical--small">
                <a href="#" class="nav-button nav-button--active click">
                    <span class="icon nav-button__icon" style="background-image:url('/svg/nav/home.svg')"></span>
                    <span class="text text--light">Home</span>
                </a>

                <a href="#" class="nav-button nav-button--width click ">
                    <span class="icon nav-button__icon" style="background-image:url('/svg/nav/orders.svg')"></span>
                    <span class="text text--light">Orders</span>
                </a>

                <a href="#" class="nav-button nav-button--width click">
                    <span class="icon nav-button__icon" style="background-image:url('/svg/nav/products.svg')"></span>
                    <span class="text text--light">Products</span>
                </a>
            </nav>
        </aside>

        <header class="header">
            <div class="layout header__content text--small">
                <div class="header__search relative">
                    <input type="text" placeholder="Search" class="text field field--header">
                    <span class="icon header__icon-search"
                        style="background-image:url('/svg/header/search.svg')"></span>
                    <span class="icon icon--header-settings icon--button icon--color-blue click"
                        style="background-image:url('/svg/header/settings.svg')"></span>
                </div>

                <div class="herader__icon-list relative">
                    <span
                        class="icon icon--header-rigt-field icon--header-signal icon--button icon--color-yellow-light click"
                        style="background-image:url('/svg/header-right/signal.svg')"></span>
                    <span
                        class="icon icon--header-rigt-field icon--header-translate icon--button icon--color-blue-light click"
                        style="background-image:url('/svg/header-right/translate.svg')"></span>
                    <span
                        class="icon icon--header-rigt-field icon--header-bell icon--button icon--color-yellow-light click"
                        style="background-image:url('/svg/header-right/bell.svg')"></span>
                </div>
            </div>
        </header>

        <main class="main">
            <div class="layout">
                <section class="box analytic__block">
                    <div class="analytic analytic--double analytic--purple">
                        <span class="icon analytic__button icon--button icon--color-purple"
                            style="background-image:url('/svg/analytic/earning.svg')">
                        </span>

                        <div class="relative analytic__content vertical vertical--small">
                            <div class="analytic__title text text--light">$500.00</div>
                            <div class="text text--small text--lighter">Total earning</div>

                            <span class="icon analytic__arrow"
                                style="background-image:url('/svg/analytic/purple-arrow.svg')">
                            </span>
                        </div>
                    </div>

                    <div class="analytic analytic--double analytic--blue">
                        <span class="icon analytic__button icon--button icon--color-blue"
                            style="background-image:url('/svg/analytic/order.svg')">
                        </span>

                        <div class="relative analytic__content vertical vertical--small">
                            <div class="analytic__title text text--light">$961</div>
                            <div class="text text--small text--lighter">Total Order</div>
                        </div>
                    </div>

                    <div class="analytic analytic__card-row analytic--blue">
                        <span class="icon analytic__button icon--button icon--color-blue"
                            style="background-image:url('/svg/analytic/earning.svg')">
                        </span>

                        <div class="relative analytic__content vertical vertical--smallest gorizontal-small ">
                            <div class="analytic__title text text--medium text--light">$203k</div>
                            <div class="text text--small text--lighter">Total Income</div>
                        </div>
                    </div>

                    <div class="analytic analytic__card-row analytic--color-none ">
                        <span class="icon analytic__button icon--button icon--color-yellow-light"
                            style="background-image:url('/svg/analytic/shop.svg')">
                        </span>

                        <div class="relative analytic__content vertical vertical--smallest gorizontal-small ">
                            <div class="analytic__title text text--medium text--dark">$203k</div>
                            <div class="text text--small text--gray">Total Income</div>
                        </div>
                    </div>
                </section>

                <section class="menu-block text">
                    <div class="box purchase__block padding--purchase-block">
                        <h3 class="text--bold text--big text--dark ">Purchase list</h3>

                        <hr class="divider">

                        <div class="purchase__list">
                            <div>
                                <p class="purchase--padding">ID437661</p>
                                <p>May 11, 2023, 14:21</p>
                            </div>
                            <div>
                                <p class="purchase--padding">AirOn ProCam</p>
                                <p>7/8 USB Type-C</p>
                            </div>
                            <div>
                                <p class="purchase--padding">132.32 EUR</p>
                                <p class="item--color-warning">Not paid</p>
                            </div>
                            <div>
                                <p class="purchase--padding">Customer</p>
                                <p>robin@com.de</p>
                            </div>
                        </div>

                        <hr class="divider">

                        <div class="purchase__list">
                            <div>
                                <p class="purchase--padding">ID437661</p>
                                <p>May 11, 2023, 14:21</p>
                            </div>
                            <div>
                                <p class="purchase--padding">PHILIPS Essential</p>
                                <p>HD9200/90</p>
                            </div>
                            <div>
                                <p class="purchase--padding">543 USD</p>
                                <p class="item--color-success">Pay by Stripe</p>
                            </div>
                            <div>
                                <p class="purchase--padding">Customer</p>
                                <p>dima@com.ua</p>
                            </div>
                        </div>
                    </div>

                    <div class="box">
                        <div class="payment__block">
                            <h3 class="text--bold text--big text--heading">Payment</h3>

                            <hr class="divider">

                            <div class="checkbox">
                                <div class="radio__item">
                                    <input type="radio" id="choice-1" name="payment" value="creditCard" checked>
                                    <div></div>
                                    <label for="choice-1" class="text--description checkbox--check">Credit card</label>
                                </div>

                                <div class="radio__item">
                                    <input type="radio" id="choice-2" name="payment" value="debitCard" class="input">
                                    <div></div>
                                    <label for="choice-2" class="text--description checkbox--check">Debit card</label>
                                </div>

                                <div class="radio__item">
                                    <input type="radio" id="choice-3" name="payment" value="payPal">
                                    <div></div>
                                    <label for="choice-3" class="text--description checkbox--check">PayPal</label>
                                </div>
                            </div>

                            <div class="input text--line">
                                <div class="input__item">
                                    <label for="cardName" class="text--description">Name on card</label>
                                    <input type="text" class="field field--aside-right" id="cardName" name="cardName">
                                </div>

                                <div class="input__item">
                                    <label for="cardNumber" class="text--description">Credit card number</label>
                                    <input maxlength="16" class="field field--aside-right" id="cardNumber"
                                        name="cardNumber">
                                </div>

                                <div class="input__item">
                                    <label for="expiration" class="text--description">Expiration</label>
                                    <input type="number" class="field field--aside-right" id="expiration"
                                        name="expiration">
                                </div>

                                <div class="input__item">
                                    <label for="cvv" class="text--description">CVV</label>
                                    <input maxlength="3" class="field field--aside-right" id="cvv" name="cvv">
                                </div>

                            </div>

                            <hr class="divider">

                            <div class="select-row">
                                <div class="select__item">
                                    <p class="text--description text--line">Country</p>
                                    <div class="relative click">
                                        <div class="field field--select">
                                            <span class="text--description">Choose...</span>
                                            <span class="icon icon--choose click"
                                                style="background-image:url('/svg/payment/select.svg')">
                                            </span>
                                        </div>
                                        <select class="field__select">
                                            <option value="country-1">America</option>
                                            <option value="country-2">Argentina</option>
                                            <option value="country-3">Spain</option>
                                            <option value="country-4">Norway</option>
                                            <option value="country-5">Australia</option>
                                        </select>
                                    </div>

                                </div>

                                <div class="select__item">
                                    <p class="text--description text--line">State</p>
                                    <div class="relative click">
                                        <div class="field field--select">
                                            <span class="text--description">Choose...</span>
                                            <span class="icon icon--choose click"
                                                style="background-image:url('/svg/payment/select.svg')">
                                            </span>
                                        </div>
                                        <select class="field__select">
                                            <option value="state-1">Alaska</option>
                                            <option value="state-2">Connecticut</option>
                                            <option value="state-3">Hawaii</option>
                                            <option value="state-4">Nevada</option>
                                            <option value="state-5">Wisconsin</option>
                                        </select>
                                    </div>

                                </div>
                            </div>

                            <hr class="divider">

                            <button class="button text--light text--line icon--button button--color click">
                                Continue to checkout
                            </button>
                        </div>
                    </div>
                </section>
            </div>
        </main>

    </div>

</body>

</html>